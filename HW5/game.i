}

void animateBarrel() {
    for (int i = 0; i < 5; i++) {
        barrel[i].aniCounter++;
        if (barrel[i].aniCounter % 15 == 0) {
            barrel[i].curFrame = (barrel[i].curFrame + 1) % barrel[i].numFrames;
        }
    }
}

