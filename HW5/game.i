void updateGame() {
    frame++;

    if (jumping) {
        if (frame - jumpFrame == 25) {
            jumping = 0;
            gravity = 10;
        }
    }

    if (!climbing && !jumping) {
        if (collisionCheck(collisionMap, 256, mario.col, mario.row, mario.width, mario.height, 0, gravity)) {
            mario.row += gravity;
        }
    }

    if (down) {
        if (mario.aniState == LEFT || mario.prevAniState == LEFT) {
            mario.aniState = LEFTDOWN;
        }
        else {
            mario.aniState = RIGHTDOWN;
        }
        if (frame % 70 == 0) {
            if (kong.collided) {
                kong.collided = 0;
            }
            down = 0;
        }
    }

    if (powerUp) {
        if (frame % 500 == 0) {
            powerUp = 0;
        }
    }


    if (mario.row == 232 || collisionCheck(laddercollisionMap, 256, mario.col, mario.row, mario.width, mario.height, 0, -mario.rdel)) {
        climbing = 0;
    }

    updateMario();
    updateKong();
    updatePauline();
    updateBarrel();
    updateHeart();
}

