void updateBarrel() {

    if (frame % 150 == 0) {
        for (int i = 0; i < 5; i++) {
            if (!barrel[i].active) {
                barrel[i].active = 1;
                break;
            }
        }
    }

    for (int i = 0; i < 5; i++) {
        if (!barrel[i].active) {
            barrel[i].row = 188;
            barrel[i].col = 240 / 2 - barrel[i].width * 3;
            barrel[i].dropped = 0;
            barrel[i].jumpedOver = 0;
        }
        else {
            if (!collisionCheck(laddercollisionMap, 256, barrel[i].col, barrel[i].row, barrel[i].width, barrel[i].height, 0, barrel[i].rdel)) {
                barrel[i].row += barrel[i].rdel;
                barrel[i].dropped = 1;
            }
            else {
                if (barrel[i].dropped) {
                    barrel[i].col -= barrel[i].cdel;
                    if (barrel[i].col + barrel[i].width - 1 < 0) {
                        barrel[i].active = 0;
                    }
                }
                else {
                    barrel[i].col += barrel[i].cdel;
                }
            }
        }
    }


    animateBarrel();
}

void updateHeart() {

    for (int i = 0; i < (3 - life); i++) {
        if (heart[i].active) {
            heart[i].active = 0;
        }
    }
}

void animateMario() {

    mario.prevAniState = mario.aniState;
    mario.aniState = IDLE;
    mario.numFrames = 2;

    if (powerUp) {
        if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4)))) {
            mario.aniState = RIGHTHAMMER;
        }
        else {
            mario.aniState = LEFTHAMMER;
        }
    }


    if(mario.aniCounter % 15 == 0) {
        mario.curFrame = (mario.curFrame + 1) % mario.numFrames;
    }


    if (!climbing && !down && !jumping) {
            if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 5))) && !powerUp) {
                mario.aniState = LEFT;
            }
            if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4))) && !powerUp) {
                mario.aniState = RIGHT;
            }
            if (!powerUp && !down && !jumping) {
                if (mario.aniState == LEFT && (!(~(oldButtons) & ((1 << 1))) && (~buttons & ((1 << 1))))) {
                    mario.aniState = LEFTJUMP;
                    jumping = 1;
                    jumpFrame = frame;
                    if (collisionCheck(collisionMap, 256, mario.col, mario.row, mario.width, mario.height, 0, -gravity)) {
                        mario.row -= gravity;
                        mario.col -= 5;
                        gravity = 0;
                    }

                }
                else if (mario.aniState == RIGHT && (!(~(oldButtons) & ((1 << 1))) && (~buttons & ((1 << 1))))) {
                    mario.aniState = RIGHTJUMP;
                    jumping = 1;
                    jumpFrame = frame;
                    if (collisionCheck(collisionMap, 256, mario.col, mario.row, mario.width, mario.height, 0, -gravity)) {
                        mario.row -= gravity;
                        mario.col += 5;
                        gravity = 0;
                    }
                }
            }

        }
        else if (!powerUp && !down && !jumping) {
            if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 6))) || (~((*(volatile unsigned short *)0x04000130)) & ((1 << 7)))) {
                mario.aniState = CLIMB;
            }
        }

    if (mario.aniState == IDLE) {
        mario.curFrame = 0;
        mario.aniCounter = 0;
        mario.aniState = mario.prevAniState;
    }
    else {
        mario.aniCounter++;
    }

}

void animateKong() {


    kong.aniCounter++;

    if(kong.aniCounter % 50 == 0) {
        kong.curFrame = (kong.curFrame + 1) % kong.numFrames;
    }

}

void animatePauline() {

